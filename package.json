{
    "name": "agent-bridge",
    "version": "1.0.0",
    "description": "A minimal Node.js + TypeScript MCP server with Express for message passing and resource locking",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "contracts:list": "node contract-cli.js list",
        "contracts:view": "node contract-cli.js view",
        "contracts:history": "node contract-cli.js history",
        "test:contracts": "node scripts/contract-smoke-test.js"
    },
    "keywords": [
        "mcp",
        "server",
        "express",
        "typescript",
        "message-bridge"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.12.2",
        "eventsource": "^1.1.2",
        "express": "^4.18.2",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@jest/transform": "^30.1.2",
        "@jest/types": "^30.0.5",
        "@types/eventsource": "^1.1.15",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.8",
        "@types/node": "^20.9.0",
        "@types/supertest": "^2.0.16",
        "jest": "^29.7.0",
        "jest-util": "^30.0.5",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    }
}
